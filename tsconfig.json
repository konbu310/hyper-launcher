{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "react",
    "outDir": "dist-tsc",
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./",
    "typeRoots": ["src/types", "node_modules/@types"]
  },
  "include": ["src/**/*"]
}
